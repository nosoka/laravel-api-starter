{
    "name": "nosoka/api",
    "description": "Base BoilerPlate for Apis",
    "keywords": [
        "laravel",
        "api"
    ],
    "require": {
        "php": ">=7.1.0",
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.3.1",
        "codeception/module-laravel": "2.0.0",
        "codeception/module-rest": "^1.2.8",
        "captbaritone/mailcatcher-codeception-module": "^2.3.1",
        "dingo/api": "^3.0",
        "illuminate/support": "^7.0|^8.0",
        "tymon/jwt-auth": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Api\\": "./src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Api\\Providers\\ApiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

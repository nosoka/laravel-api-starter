{
    "name": "nosoka/api",
    "description": "Base BoilerPlate for Apis",
    "keywords": [
        "laravel",
        "api"
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "^7.0|^8.0",
        "dingo/api": "^3.0",
        "tymon/jwt-auth": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Api\\" : "./src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Api\\Providers\\ApiServiceProvider"            ]
        }
    }
}
